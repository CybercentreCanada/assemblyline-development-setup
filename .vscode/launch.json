{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "[Core] CLI - Command line interface",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-base/assemblyline/run/cli.py",
            "console": "integratedTerminal"
        },
        {
            "name": "[Core] Alerter",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-core/assemblyline_core/alerter/run_alerter.py",
            "console": "internalConsole"
        },
        {
            "name": "[Core] Dispatcher",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-core/assemblyline_core/dispatching/__main__.py",
            "env": {
                "SKIP_SERVICE_SETUP": "true"
            },
            "console": "internalConsole"
        },
        {
            "name": "[Core] Expiry",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-core/assemblyline_core/expiry/run_expiry.py",
            "console": "internalConsole"
        },
        {
            "name": "[Core] Ingester",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-core/assemblyline_core/ingester/__main__.py",
            "console": "internalConsole"
        },
        {
            "name": "[Core] Metrics Heartbeat",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-core/assemblyline_core/metrics/run_heartbeat_manager.py",
            "console": "internalConsole"
        },
        {
            "name": "[Core] Metrics Aggregator",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-core/assemblyline_core/metrics/run_metrics_aggregator.py",
            "console": "internalConsole"
        },
        {
            "name": "[Core] Scaler",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-core/assemblyline_core/scaler/run_scaler.py",
            "env": {
                "FILE_UPDATE_VOLUME": "/tmp/update_data",
                "FILE_UPDATE_DIRECTORY": "/tmp/update_data"
            },
            "console": "internalConsole"
        },
        {
            "name": "[Core] Statistics Aggregator",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-core/assemblyline_core/metrics/run_statistics_aggregator.py",
            "console": "internalConsole"
        },
        {
            "name": "[Core] Updater",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-core/assemblyline_core/updater/run_updater.py",
            "env": {
                "FILE_UPDATE_VOLUME": "/tmp/update_data",
                "FILE_UPDATE_DIRECTORY": "/tmp/update_data"
            },
            "console": "internalConsole"
        },
        {
            "name": "[Core] Workflow",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-core/assemblyline_core/workflow/run_workflow.py",
            "console": "internalConsole"
        },
        {
            "name": "[DATA] Create default users",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-base/assemblyline/odm/random_data/create_test_data.py",
            "args": [
                "nosvc",
                "nosigs"
            ],
            "console": "internalConsole"
        },
        {
            "name": "[DATA] Generate random data",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-base/assemblyline/odm/random_data/create_test_data.py",
            "args": [
                "full"
            ],
            "console": "internalConsole"
        },
        {
            "name": "[Service Server] API Server",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-service-server/assemblyline_service_server/app.py",
            "console": "internalConsole"
        },
        {
            "name": "[Service] ResultSample - LIVE",
            "type": "python",
            "request": "launch",
            "program": "../assemblyline_v4_service/run_service.py",
            "env": {
                "SERVICE_PATH": "assemblyline_result_sample_service.result_sample.ResultSample"
            },
            "console": "internalConsole",
            "cwd": "${workspaceFolder}/assemblyline-v4-service/assemblyline_result_sample_service"
        },
        {
            "name": "[Service] TaskHandler",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-service-client/assemblyline_service_client/task_handler.py",
            "console": "internalConsole"
        },
        {
            "name": "[UI] API Server",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-ui/assemblyline_ui/app.py",
            "console": "internalConsole"
        },
        {
            "name": "[UI] Socket Server",
            "gevent": true,
            "type": "python",
            "request": "launch",
            "program": "assemblyline-ui/assemblyline_ui/socketsrv.py",
            "console": "internalConsole"
        },
        {
            "name": "[Kubernetes][Core] Alerter",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-core/assemblyline_core/alerter/run_alerter.py",
            "console": "internalConsole",
            "preLaunchTask": "bridge-to-kubernetes.alerter"
        },
        {
            "name": "[Kubernetes][Core] Dispatcher",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-core/assemblyline_core/dispatching/__main__.py",
            "env": {
                "SKIP_SERVICE_SETUP": "true"
            },
            "console": "internalConsole",
            "preLaunchTask": "bridge-to-kubernetes.dispatcher"
        },
        {
            "name": "[Kubernetes][Core] Expiry",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-core/assemblyline_core/expiry/run_expiry.py",
            "console": "internalConsole",
            "preLaunchTask": "bridge-to-kubernetes.expiry"
        },
        {
            "name": "[Kubernetes][Core] Ingester",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-core/assemblyline_core/ingester/__main__.py",
            "console": "internalConsole",
            "preLaunchTask": "bridge-to-kubernetes.ingester"
        },
        {
            "name": "[Kubernetes][Core] Scaler",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-core/assemblyline_core/scaler/run_scaler.py",
            "env": {
                "KUBECONFIG": "~/.kube/al.config",
                "FILE_UPDATE_VOLUME": "/tmp/update_data",
                "FILE_UPDATE_DIRECTORY": "/tmp/update_data"
            },
            "console": "internalConsole",
            "preLaunchTask": "bridge-to-kubernetes.scaler"
        },
        {
            "name": "[Kubernetes][Core] Updater",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-core/assemblyline_core/updater/run_updater.py",
            "env": {
                "KUBECONFIG": "~/.kube/al.config",
                "FILE_UPDATE_VOLUME": "/tmp/update_data",
                "FILE_UPDATE_DIRECTORY": "/tmp/update_data"
            },
            "console": "internalConsole",
            "preLaunchTask": "bridge-to-kubernetes.updater"
        },
        {
            "name": "[Kubernetes][Core] Workflow",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-core/assemblyline_core/workflow/run_workflow.py",
            "console": "internalConsole",
            "preLaunchTask": "bridge-to-kubernetes.workflow"
        },
        {
            "name": "[Kuberenetes][Service Server] API Server",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-service-server/assemblyline_service_server/app.py",
            "console": "internalConsole",
            "preLaunchTask": "bridge-to-kubernetes.service-server"
        },
        {
            "name": "[Kubernetes][UI] API Server",
            "type": "python",
            "request": "launch",
            "program": "assemblyline-ui/assemblyline_ui/app.py",
            "console": "internalConsole",
            "preLaunchTask": "bridge-to-kubernetes.ui"
        },
        {
            "name": "[Kubernetes][UI] Socket Server",
            "gevent": true,
            "type": "python",
            "request": "launch",
            "program": "assemblyline-ui/assemblyline_ui/socketsrv.py",
            "console": "internalConsole",
            "preLaunchTask": "bridge-to-kubernetes.socketio"
        }
    ]
}
